{
	"env":
	{
		"es6": true,
		"node": true
	},
	"extends":
	[
		"@isnotdefined/eslint-config"
	],
	"parserOptions":
	{
		"sourceType": "module",
		"ecmaFeatures": {
            "jsx": true
        },
		"ecmaVersion": 2018
	},

	"rules": {
        "jsdoc/require-jsdoc": "off",
		"jsdoc/require-param-type": "off",
		"jsdoc/require-description": "off",
		"jsdoc/jsdoc/require-description": "off",
		"jsdoc/require-returns": "off"
	}
}