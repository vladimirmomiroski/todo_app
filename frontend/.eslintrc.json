{
	
	"env":
	{
		"es6": true,
		"node": true
	},
	"extends":
	[
		"@isnotdefined/eslint-config"
	],
	"parserOptions":
	{
		"sourceType": "module",
		"ecmaFeatures": {
            "jsx": true
        },
		"ecmaVersion": 2018
	},
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
	}
	
}